[{"data":1,"prerenderedAt":416},["ShallowReactive",2],{"language":3,"aside-major-nav":47,"aside-minor":222,"content-data":266,"contributor-count":289,"external-api":290,"main-bitran-content":352},{"strFunctions":4,"strPhrases":6},{"m":5},"return function m(\nnumber,\none,\ntwo,\nfive,\nincludeNumber = true)\n{\n  const text = [five, one, two, two, two, five][\n  number % 100 > 10 && number % 100 \u003C 20 ? 0 : Math.min(number % 10, 5)];\n\n  return includeNumber ? `${number} ${text}` : text;\n}",{"site_info_title":7,"site_info_slogan":8,"index":9,"main_page":10,"_language_code":11,"theme":12,"pages":13,"contributors":14,"_language_title":15,"theme_system":16,"content":17,"search":18,"add_translation":19,"theme_light":20,"language":21,"theme_dark":22,"other":23,"article":24,"empty_toc":25,"make_contribution":26,"summary":27,"material_improvement":28,"empty_nav":29,"practice":30,"edit_page":31,"how_to_improve":32,"report_problem":33,"to_index":34,"about_book":35,"seo_article_description":36,"seo_summary_description":37,"seo_practice_description":38,"flag_dev":39,"flag_dev_description":40,"flag_advanced":41,"flag_advanced_description":42,"flag_secondary":43,"flag_secondary_description":44,"popover_dependencies":45,"popover_dependencies_description":46},"Erudit","Современный учебник","Оглавление","Главная страница","ru","Тема","Страницы","Авторы","Русский","Системная","Контент","Поиск","Добавить перевод","Светлая","Язык","Темная","Другое","Статья","Пустая таблица содержимого...","Внести свой вклад","Конспект","Улучшение материала","Пустая навигация","Задачи","Редактировать страницу","Как улучшить?","Сообщить о проблеме","К оглавлению","Об учебнике","~!~FUNC~!~return (contentTitle) =>\n  `Понятное и интересное объяснение темы «${contentTitle}». Показательные примеры, важные свойства, интересные факты, применение в жизни, понятные доказательства. Здесь вы точно разберетесь!`","~!~FUNC~!~return (contentTitle) =>\n  `Конспект темы «${contentTitle}»: ключевые определения, т