math/lang-of-math/math-lang":252},{"type":64,"generic":253,"bookTitle":201,"topicPartLinks":273},{"contentId":193,"type":64,"title":194,"description":254,"flags":255,"previousNext":256,"context":259,"seo":72,"ogImage":72,"references":72},"Математические языки, почему в них входит язык математики, а также чем они отличаются от естественных языков.",{},{"previous":257,"next":258},{"title":191,"link":248},{"title":197,"link":250},[260,264,266,270],{"type":261,"icon":90,"title":221,"href":262,"hidden":263},"Группа","/group/archive",false,{"type":261,"icon":90,"title":165,"href":265,"hidden":263},"/group/archive/base-math",{"type":267,"icon":268,"title":201,"href":269,"hidden":263},"Книга","outline/book","/book/archive/base-math/lang-of-math",{"type":12,"icon":271,"title":194,"href":272,"hidden":263},"outline/file-lines","/topic/archive/base-math/lang-of-math/math-lang",{"article":239},5,{"repository":276,"languages":331},{"name":277,"commit":278,"_links":322,"protected":263,"protection":325,"protection_url":330},"main",{"sha":279,"node_id":280,"commit":281,"url":294,"html_url":295,"comments_url":296,"author":297,"committer":316,"parents":317},"013ed07a6ba09c49a3ca162eb2bcef5b5d33c1e8","C_kwDOOGzAodoAKDAxM2VkMDdhNmJhMDljNDlhM2NhMTYyZWIyYmNlZjViNWQzM2MxZTg",{"author":282,"committer":286,"message":287,"tree":288,"url":291,"comment_count":67,"verification":292},{"name":283,"email":284,"date":285},"Peter Radko","cmtvpetya@gmail.com","2025-05-19T07:59:52Z",{"name":283,"email":284,"date":285},"Обновление до Erudit 3.0.0-dev.20",{"sha":289,"url":290},"2fcc171f468883522f1f2bb43640bca6d9c8d204","https://api.github.com/repos/open-math/ru.omath.net/git/trees/2fcc171f468883522f1f2bb43640bca6d9c8d204","https://api.github.com/repos/open-math/ru.omath.net/git/commits/013ed07a6ba09c49a3ca162eb2bcef5b5d33c1e8",{"verified":263,"reason":293,"signature":72,"payload":72,"verified_at":72},"unsigned","https://api.github.com/repos/open-math/ru.omath.net/commits/013ed07a6ba09c49a3ca162eb2bcef5b5