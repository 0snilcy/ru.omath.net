[{"data":1,"prerenderedAt":425},["ShallowReactive",2],{"language":3,"aside-major-nav":48,"aside-minor":225,"content-data":253,"external-api":275,"contributor-count":337,"sponsor-count":338,"main-bitran-content":339},{"strFunctions":4,"strPhrases":6},{"m":5},"return function m(\nnumber,\none,\ntwo,\nfive,\nincludeNumber = true)\n{\n  const text = [five, one, two, two, two, five][\n  number % 100 > 10 && number % 100 \u003C 20 ? 0 : Math.min(number % 10, 5)];\n\n  return includeNumber ? `${number} ${text}` : text;\n}",{"site_info_title":7,"site_info_slogan":8,"index":9,"main_page":10,"_language_code":11,"theme":12,"pages":13,"contributors":14,"_language_title":15,"theme_system":16,"content":17,"search":18,"sponsors":19,"add_translation":20,"theme_light":21,"language":22,"theme_dark":23,"other":24,"article":25,"empty_toc":26,"make_contribution":27,"summary":28,"material_improvement":29,"practice":30,"edit_page":31,"how_to_improve":32,"report_problem":33,"empty_nav":34,"to_index":35,"about_book":36,"seo_article_description":37,"seo_summary_description":38,"seo_practice_description":39,"flag_dev":40,"flag_dev_description":41,"flag_advanced":42,"flag_advanced_description":43,"flag_secondary":44,"flag_secondary_description":45,"popover_dependencies":46,"popover_dependencies_description":47},"Erudit","Современный учебник","Оглавление","Главная страница","ru","Тема","Страницы","Авторы","Русский","Системная","Контент","Поиск","Спонсоры","Добавить перевод","Светлая","Язык","Темная","Другое","Статья","Пустая таблица содержимого...","Внести свой вклад","Конспект","Улучшение материала","Задачи","Редактировать страницу","Как улучшить?","Сообщить о проблеме","Пустая навигация","К оглавлению","Об учебнике","~!~FUNC~!~return (contentTitle) =>\n  `Понятное и интересное объяснение темы «${contentTitle}». Показательные примеры, важные свойства, интересные факты, применение в жизни, понятные доказательства. Здесь вы точно разберетесь!`","~!~FUNC~!~return (contentTitle) =>\n  `Конспект те