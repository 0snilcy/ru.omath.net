ions_url":324,"repos_url":325,"events_url":326,"received_events_url":327,"type":328,"user_view_type":329,"site_admin":279},[332],{"sha":333,"url":334,"html_url":335},"e5cb1bd4ea977025a0b7a505aeeab941607470cc","https://api.github.com/repos/open-math/ru.omath.net/commits/e5cb1bd4ea977025a0b7a505aeeab941607470cc","https://github.com/open-math/ru.omath.net/commit/e5cb1bd4ea977025a0b7a505aeeab941607470cc",{"self":337,"html":338},"https://api.github.com/repos/open-math/ru.omath.net/branches/main","https://github.com/open-math/ru.omath.net/tree/main",{"enabled":279,"required_status_checks":340},{"enforcement_level":341,"contexts":342,"checks":343},"off",[],[],"https://api.github.com/repos/open-math/ru.omath.net/branches/main/protection",{"en":346,"ru":349},{"name":347,"link":348},"English","https://en.omath.net",{"name":15,"link":350},"https://omath.ru",6,3,{"biCode":354,"storage":355,"routes":369,"context":370,"mainLocation":373},"# Дробь\n\nЛюди используют целые числа для обозначения количества объектов: 100 баллов на экзамене, 53 овцы на ферме, 4 стула за столом, 1 мозг в голове.\nОднако, часто требуется разбить/разрезать/поделить один целый объект на одинаковые части.\n\nДля обозначения частей одного объекта требуется указать, на сколько долей был разделен объект и сколько долей мы \"взяли\".\nТакой новый вид чисел назвали дробями (означает дробление, деление, разделение).\n\n@include\n    location: article|fraction\n    resolved: true\n    blocks: |\n        { #summary|archive/basics/fraction/definition|include:ghijkl__summary|archive/basics/fraction/definition|include:ghijkl }\n        @term\n            title: Дробь\n            main: |\n                { #summary|archive/basics/fraction/definition|include:ghijkl__summary|archive/basics/fraction/definition|include:ghijkl }\n                Число, обозначающее часть целого объекта.\n\n# Виды дробей\n\nДроби можно описывать словестно (\"половина\", \"2 части из 3\", \"15 долей из 30\"), а можно изображать графически с помощью частично закрашенных окружностей.\nОба этих способа неудобно использовать в рассчетах.\nПоэтому были придуманы два удобных способа записи дробей через цифры: обыкновенные и десятичные дроби.\n\n@include\n    location: article|fraction-types\n    resolved: true\n    blocks: |\n        { #summary|archive/basics/fraction/definition|include:CDEFGH__summary|archive/basics/fraction/definition|include:CDEFGH }\n        @image\n            src: /archive/basics/fraction/definition/assets/fraction-types.svg",{"image:/archive/basics/fraction/definition/assets/fraction-types.svg":356,"accent:term":363},{"type":357,"data":358},"success",{"resolvedSrc":359,"size":360},"99-archive/01-basics/05-fraction/1-definition/assets/fraction-types.svg",{"width":361,"height":362},382,256,{"type":357,"data":364},{"colors":365},{"text":366,"background":367,"border":368},"light-dark( #65a63c, #a4c965)","light-dark( #f8feef, #2a3123)","light-dark( #dde7c9, #404b2d)",[],{"location":371,"aliases":372},{"type":251,"path":195},{},{"type":251,"path":374},"archive/basics/fraction/definition/",1752139116856]