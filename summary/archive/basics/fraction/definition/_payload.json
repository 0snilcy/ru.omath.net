3},"archive/base-math/base-logic/about","О математической логике",{"type":63,"part":64,"id":195,"fullId":195,"level":67,"flags":68,"label":196},"archive/base-math/base-logic/propositions","Высказывания","Основы логики",{"type":58,"children":199,"id":52,"fullId":52,"level":165,"flags":68,"label":147},[200,203],{"type":63,"part":64,"id":201,"fullId":201,"level":67,"flags":68,"label":202},"archive/base-math/algebra/monotonic","Монотонная функция",{"type":63,"part":64,"id":204,"fullId":204,"level":67,"flags":68,"label":205},"archive/base-math/algebra/abs","Модуль","archive/base-math","archive","Архив",{"/summary/archive/basics/fraction/definition":210},{"type":63,"topicId":159,"location":211,"toc":213,"nav":221,"contributors":229},{"type":212,"path":159},"summary",[214,218],{"id":215,"productName":216,"level":67,"title":217},"drob","heading","Дробь",{"id":219,"productName":216,"level":67,"title":220},"vidy-drobey","Виды дробей",{"previous":222,"next":224,"article":226,"summary":227,"practice":228},{"title":170,"link":223},"/group/archive/basics/fraction",{"title":168,"link":225},"/group/archive/basics/fraction/simple","/article/archive/basics/fraction/definition","/summary/archive/basics/fraction/definition","/practice/archive/basics/fraction/definition",[230],{"contributorId":231,"displayName":232,"avatar":233},"gwynerva","Петр Радько","gwynerva/avatar.png",{"archive/basics/fraction/definition":235},{"type":63,"generic":236,"bookTitle":151,"topicPartLinks":255},{"contentId":159,"type":63,"title":160,"description":237,"flags":238,"previousNext":239,"context":242,"seo":68,"ogImage":68,"references":68},"Разбираемся, что такое дроби, учимся делить целое на части, а также рисовать частично закрашенные круги.",{},{"previous":240,"next":241},{"title":170,"link":223},{"title":168,"link":225},[243,247,251,252],{"type":244,"icon":75,"title":208,"href":245,"hidden":246},"Группа","/group/archive",false,{"type":248,"icon":249,"title":171,"href":250,"hidden":246},"Книга","outline/book","/book/archive/basics",{"type":244,"icon":75,"title":170,"href":223,"hidden":246},{"type":11,"icon":253,"title":160,"href":254,"hidden":246},"outline/file-lines","/topic/archive/basics/fraction/definition",{"article":226,"summary":227,"practice":228},5,{"biCode":258,"renderDataStorage":259,"location":273},"# Дробь\n\nЛюди используют целые числа для обозначения количества объектов: 100 баллов на экзамене, 53 овцы на ферме, 4 стула за столом, 1 мозг в голове.\nОднако, часто требуется разбить/разрезать/поделить один целый объект на одинаковые части.\n\nДля обозначения частей одного объекта требуется указать, на сколько долей был разделен объект и сколько долей мы \"взяли\".\nТакой новый вид чисел назвали дробями (означает дробление, деление, разделение).\n\n@include\n    location: article|fraction\n    resolved: true\n    blocks: |\n        { #summary|archive/basics/fraction/definition|include:ghijkl__summary|archive/basics/fraction/definition|include:ghijkl }\n        @term\n            title: Дробь\n            main: |\n                { #summary|archive/basics/fraction/definition|include:ghijkl__summary|archive/basics/fraction/definition|include:ghijkl }\n                Число, обозначающее часть целого объекта.\n\n# Виды дробей\n\nДроби можно описывать словестно (\"половина\", \"2 части из 3\", \"15 долей из 30\"), а можно изображать графически с помощью частично закрашенных окружностей.\nОба этих способа неудобно использовать в рассчетах.\nПоэтому были придуманы два удобных способа записи дробей через цифры: обыкновенные и десятичные дроби.\n\n@include\n    location: article|fraction-types\n    resolved: true\n    blocks: |\n        { #summary|archive/basics/fraction/definition|include:CDEFGH__summary|archive/basics/fraction/definition|include:CDEFGH }\n        @image\n            src: /archive/basics/fraction/definition/assets/fraction-types.svg",{"image:/archive/basics/fraction/definition/assets/fraction-types.svg":260,"accent:term":267},{"type":261,"data":262},"success",{"resolvedSrc":263,"size":264},"99-archive/01-basics/05-fraction/1-definition/assets/fraction-types.svg",{"width":265,"height":266},382,256,{"type":261,"data":268},{"colors":269},{"text":270,"background":271,"border":272},"light-dark( #65a63c, #a4c965)","light-dark( #f8feef, #2a3123)","light-dark( #dde7c9, #404b2d)","summary┃archive╱basics╱fraction╱definition",1746638085835]